using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace homeWork2_3
{
    class Program
    {
        static void Main(string[] args)
        {
            int initialNumber = 5;
            int finiteNumber = 96;
            int numberChangeStep = 7;

            for (int i = initialNumber; i <= finiteNumber; i += numberChangeStep)
            {
                Console.WriteLine(i);
            }
        }
    }
}

